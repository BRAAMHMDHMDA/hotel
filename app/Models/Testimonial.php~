<?php

namespace App\Models;

use App\Traits\HasImage;
use Illuminate\Database\Eloquent\Factories\HasFactory;
use Illuminate\Database\Eloquent\Model;

class Testimonial extends Model
{
    use HasImage;


    protected $fillable = [
        'name',
        'city',
        'image_path',
        'message',
    ];

}
